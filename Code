int led1 = D7; 

void setup() {
  pinMode(led1, OUTPUT);
}


void loop() {

  digitalWrite(led1, HIGH);
  delay(20);


  digitalWrite(led1, LOW);
  delay(10);
  
  
  digitalWrite(led1, LOW);
  delay(10);
  
  digitalWrite(led1, HIGH);
  delay(20);
 
  digitalWrite(led1, LOW);
  delay(10);

  digitalWrite(led1, LOW);
  delay(10);
  

  digitalWrite(led1, LOW);
  delay(10);
  
  digitalWrite(led1, HIGH);
  delay(20);
  
  digitalWrite(led1, HIGH);
  delay(20);
  
  digitalWrite(led1, LOW);
  delay(10);
 
  digitalWrite(led1, LOW);
  delay(10);

  digitalWrite(led1, LOW);
  delay(10);

  digitalWrite(led1, LOW);
  delay(10);
  digitalWrite(led1, LOW);
  delay(10);
  digitalWrite(led1, HIGH);
  delay(20);
  digitalWrite(led1, HIGH);
  delay(20);

  digitalWrite(led1, LOW);
  delay(10);
  
}

